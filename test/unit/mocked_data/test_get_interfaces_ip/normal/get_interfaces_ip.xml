<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0" xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0">
    <configure xmlns="urn:nokia.com:sros:ns:yang:sr:conf">
        <router>
            <router-name>Base</router-name>
            <interface>
                <interface-name>system</interface-name>
                <ipv4>
                    <primary>
                        <address>1.1.1.1</address>
                        <prefix-length>32</prefix-length>
                    </primary>
                </ipv4>
            </interface>
            <interface>
                <interface-name>vlan901</interface-name>
                <ipv6>
                    <address>
                        <ipv6-address>2400:cb00:133:900::1</ipv6-address>
                        <prefix-length>64</prefix-length>
                    </address>
                </ipv6>
            </interface>
        </router>
        <service>
            <vprn>
                <service-name>2100</service-name>
                <interface>
                    <interface-name>to_Network</interface-name>
                    <ipv4>
                        <primary>
                            <address>172.16.1.1</address>
                            <prefix-length>24</prefix-length>
                        </primary>
                    </ipv4>
                </interface>
            </vprn>
        </service>
    </configure>
</data>
